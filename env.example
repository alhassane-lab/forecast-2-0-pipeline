# ============================================================================
# FILE: .env.example
# ============================================================================
# Variables d'environnement pour le pipeline Forecast 2.0
# Copier ce fichier vers .env et remplir avec vos vraies valeurs

# AWS Configuration
AWS_REGION=eu-west-1
AWS_DEFAULT_REGION=eu-west-1
AWS_PROFILE=default
S3_RAW_BUCKET=greenandcoop-raw-data
S3_PROCESSED_BUCKET=greenandcoop-processed-data

# MongoDB Configuration
# ECS private replica set example (works from ECS tasks / hosts inside VPC):
# MONGODB_URI=mongodb://mongo-1.mongo.internal:27017,mongo-2.mongo.internal:27017,mongo-3.mongo.internal:27017/forecast_2_0?replicaSet=rs0
MONGODB_URI=
MONGODB_DATABASE=forecast_2_0
MONGODB_COLLECTION=weather_measurements
MONGODB_TLS=
MONGODB_TLS_ALLOW_INVALID_CERTS=

# Airbyte Configuration (optionnel)
AIRBYTE_URL=
AIRBYTE_USERNAME=airbyte
AIRBYTE_PASSWORD=password

# Pipeline Configuration
PIPELINE_LOG_LEVEL=INFO
PIPELINE_STRICT_MODE=false
